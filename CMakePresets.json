{
    "version": 6,
    "configurePresets": [
        {
            "name": "stm32-nucleo-base",
            "hidden": true,
            "generator": "Unix Makefiles",
            "binaryDir": "${sourceDir}/_out/${presetName}",
            "toolchainFile": "${sourceDir}/Build/${presetName}/Toolchain.cmake",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "LINKER_SCRIPT": "${sourceDir}/${presetName}/Main/Source/LinkerScript.ld"
            }
        },
        {
            "name": "G491RE",
            "inherits": "stm32-nucleo-base",
            "cacheVariables": {
                "BOARD": "${presetName}",
                "EXECUTABLE_NAME": "NucleoFirmware"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "G491RE",
            "configurePreset": "G491RE",
            "targets": ["NucleoFirmware"]
        }
    ]
}
